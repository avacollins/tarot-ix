{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "esnext",
    "noEmit": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "paths": {
      "@tarot-ix/*": [
        "packages/*"
      ]
    }
  },
  "references": [
    {
      "path": "apps/north-east-west-south"
    },
    {
      "path": "apps/tarot"
    }
  ],
  "exclude": [
    "node_modules",
    "*.mjs"
  ]
}